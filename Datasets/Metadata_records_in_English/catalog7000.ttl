        <http://sws.geonames.org/3337387/> ;
    dcterms:title "DEMNA-DNE : Exotic animal occurrences in Wallonia, Belgium"@en ;
    dcat:accessURL <http://data.biodiversity.be/dataset/b483c4d9-efe9-4cde-a452-0c74b85cb8d9> ;
    dcat:theme <http://publications.europa.eu/resource/authority/data-theme/ENVI> .

